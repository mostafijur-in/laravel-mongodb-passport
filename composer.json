{
    "name": "mostafijur-in/laravel-mongodb-passport",
    "description": "A package to allow laravel/passport use with jenssegers/laravel-mongodb",
    "homepage": "https://github.com/mostafijur-in/laravel-mongodb-passport",
    "license": "MIT",
    "keywords": [
        "laravel",
        "laravel-mongodb",
        "laravel-mongodb-passport",
        "laravel-passport",
        "mongodb",
        "passport",
        "designmynight"
    ],
    "require": {
        "php": "^8.0.2",
        "illuminate/support": "^9.0",
        "jenssegers/mongodb": "^3.9",
        "laravel/passport": "^11.5"
    },
    "autoload": {
        "psr-4": {
            "DesignMyNight\\Mongodb\\": "src"
        }
    },
    "authors": [
        {
            "name": "Mostafijur",
            "email": "mostafijur.init@gmail.com",
            "role": "Support"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "DesignMyNight\\Mongodb\\MongodbPassportServiceProvider"
            ]
        }
    }
}
